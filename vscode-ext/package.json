{
    "name": "rel",
    "displayName": "REL - Requirements Engineering Language",
    "description": "REL - A Domain Specific Language for Requirements Engineering",
    "version": "0.9.0",
    "publisher": "sscit",
    "author": {
        "name": "Stefan Schlichth√§rle"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "rel",
                "aliases": [
                    "Requirements Engineering Language",
                    "rel"
                ],
                "extensions": [
                    ".rd",
                    ".rs"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "rel",
                "scopeName": "text.rel",
                "path": "./syntaxes/rel.tmLanguage.json"
            }
        ],
        "themes": [
            {
                "label": "RELTheme",
                "uiTheme": "vs-dark",
                "path": "./themes/reltheme.tmTheme"
            }
        ]
    },
    "activationEvents": [
        "*"
    ],
    "main": "./extension.js",
    "dependencies": {
        "vscode-languageclient": "^7.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sscit/rel.git"
    },
    "homepage": "https://github.com/sscit/rel/blob/main/README.md"
}
